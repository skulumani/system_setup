snippet subfigure
abbr    subfigure
options head, indent
        \begin{subfigure}[htbp]{${1}\textwidth} 
                \includegraphics[width=\textwidth, keepaspectratio]{${2}} 
                \caption{${3} \label{fig:${4}}} 
        \end{subfigure} 
        ${0}

snippet subcaptionbox
abbr    subcaptionbox
options head, indent
        \subcaptionbox{${1:Caption}\label{fig:${2:label}}}{\includegraphics[width=0.5\textwidth]{${3}}}
        ${0}
